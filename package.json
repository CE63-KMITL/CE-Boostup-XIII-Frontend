{
  "name": "ce-boostup-xiii-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": "CE63@KMITL",
  "license": "MIT",
  "scripts": {
    "<--Main-->": "",
    "dev": "vite dev",
    "run": "npm run build && node build",
    "build > create-image > run": "npm-run-all stop-container create-image run-container",
    "<--Docker-->": "",
    "run-container": "echo 'Server running at : \u001b[32mhttp://localhost:3001\u001b[39m' && docker run -p 3001:3001 ce-boostup-xiii-frontend",
    "stop-container": "docker ps -a --filter \"ancestor=ce-boostup-xiii-frontend\" -q | xargs -r docker rm -f || true",
    "create-image": "docker build -t ce-boostup-xiii-frontend .",
    "<--Svelte-->": "",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@types/node": "^22.14.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "sass-embedded": "^1.86.3",
    "svelte": "^5.28.1",
    "svelte-check": "^4.1.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^5.4.18"
  },
  "dependencies": {
    "ioredis": "^5.6.1",
    "markdown-it": "^14.1.0",
    "monaco-editor": "^0.52.2",
    "monaco-themes": "^0.4.4",
    "rate-limiter-flexible": "^7.1.1",
    "svelte-icons": "^2.1.0"
  }
}